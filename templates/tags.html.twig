<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>Document</title>
	</head>
	<body>

		{% for message in app.flashes('notice') %}
			<h4>{{ message }}</h4>
		{% endfor %}

		{% for tag in tags %}
			<h2>{{ tag.name }}</h2>
			<h3>{{ tag.description }}</h3>
		{% endfor %}
	</body>
</html>
